{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "transform-runtime",
      {
        "polyfill": false
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "common": "./src/common",
          "components": "./src/components",
          "containers": "./src/containers",
          "lib": "./src/lib",
          "srcredux": "./src/redux"
        }
      }
    ]
  ]
}